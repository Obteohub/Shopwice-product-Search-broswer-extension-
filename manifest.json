{
  "manifest_version": 3,
  "name": "Shopwice Quick Search & Deals",
  "version": "1.0",
  "description": "Instant product search and the best deals from Shopwice.com.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
